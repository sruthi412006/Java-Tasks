import java.util.Scanner;
public class Strongnumber{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int original = n;
    int r;
    int sum = 0;
    while(n!=0){
      r = n % 10;
      int fact = 1;
      for(int i=1 ; i<=n ; i++){
        fact*=1;
      }
      sum+=fact;
      n/10;
    }
    System.out.print(sum==original)
      }
}
