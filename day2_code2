import java.util.Scanner;
public class Spynumber{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int sum = 0;
    int prod = 1;
    while(n!=0){
      num = n%10;
      sum += num;
      prod *= num;
      n /=10;
    }
    System.out.println(sum==prod);
  }
}
