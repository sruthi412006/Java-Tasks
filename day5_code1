import java.util.Scanner;
public class Neonnumber{
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int m = n*n;
    int sum = 0;
    while(m != 0){
      int mod = m % 10;
      sum += mod;
      m = m/10;
    }
    if(n == sum)
      System.out.println("Neon number");
    else
      System.out.println("Not a neon number");
      }
}
